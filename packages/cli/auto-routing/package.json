    "publish": "pnpm publish --no-git-checks --access public"
  },
  "bin": {
    "auto-routing": "./dist/index.js"
  },
  "keywords": [],
  "author": "jeongwoo903",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.12",
    "chalk": "^5.4.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.4",
    "tsup": "^8.4.0"
  }
}
