{
  "name": "@mash-up-web-toolkit/auto-routing",
  "version": "0.0.1",
  "description": "Generate auto routing constants file used by @mash-up-web-toolkit/auto-routing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --",
