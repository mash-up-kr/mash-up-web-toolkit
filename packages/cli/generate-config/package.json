{
  "name": "@mash-up-web-toolkit/generate-config",
  "version": "0.0.9",
  "description": "Create config for @mash-up-web-toolkit/command",
  "author": "brightbong92",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mash-up-kr/mash-up-web-toolkit.git",
    "directory": "packages/cli/generate-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "publish": "pnpm publish --no-git-checks --access public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@mash-up-web-toolkit/eslint-config": "workspace:*",
    "@types/node": "^22.13.10",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "latest"
  }
}
