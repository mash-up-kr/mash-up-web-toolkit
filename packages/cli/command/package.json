{
  "name": "@mash-up-web-toolkit/command",
  "version": "0.0.18",
  "description": "mash-up-web-toolkit command line interface",
  "author": "brightbong92",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mash-up-kr/mash-up-web-toolkit.git",
    "directory": "packages/cli/command"
  },
  "bin": {
    "mash-up-web": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "dev": "pnpm build && node dist/index.js",
    "publish": "pnpm publish --no-git-checks --access public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "keywords": [],
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.13.10",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "@mash-up-web-toolkit/eslint-config": "workspace:*"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@mash-up-web-toolkit/generate-api": "workspace:*",
    "@mash-up-web-toolkit/generate-config": "workspace:*",
    "@mash-up-web-toolkit/generate-api-config": "workspace:*",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "esbuild": "^0.25.2",
    "figlet": "^1.8.0",
    "inquirer": "^12.5.0"
  }
}
